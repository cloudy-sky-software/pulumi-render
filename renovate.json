{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "ignoreDeps": [
    "github.com/mattn/go-ieproxy"
  ],
  "ignorePaths": [
    "sdk/**",
    "examples/**"
  ],
  "hostRules": [
    {
      "matchHost": "https://github.com/cloudy-sky-software",
      "hostType": "go",
      "encrypted": {
        "token": ""
      }
    }
  ],
  "customEnvVariables": {
    "GOPRIVATE": "github.com/cloudy-sky-software/*"
  },
  "postUpdateOptions": [
    "gomodTidy"
  ]
}